    {"id": "grantee_name", "label": {"uzbek": "Oluvchi tomon F.I.Sh./Nomi", "russian": "Ф.И.О./Наименование получателя"}, "type": "text", "required": true},
    {"id": "grantee_passport", "label": {"uzbek": "Pasport/STIR", "russian": "Паспорт/ИНН"}, "type": "text", "required": true},
    {"id": "land_location", "label": {"uzbek": "Yer uchastkasi joylashuvi", "russian": "Местонахождение земельного участка"}, "type": "text", "required": true},
    {"id": "cadastral_number", "label": {"uzbek": "Kadastr raqami", "russian": "Кадастровый номер"}, "type": "text", "required": true},
    {"id": "land_area", "label": {"uzbek": "Maydoni (gektar yoki kv.m)", "russian": "Площадь (га или кв.м)"}, "type": "number", "required": true, "validation": {"min": 0}},
    {"id": "land_purpose", "label": {"uzbek": "Maqsadi", "russian": "Целевое назначение"}, "type": "select", "required": true, "options": [{"value": "agriculture", "label": {"uzbek": "Qishloq xo'jalik", "russian": "Сельское хозяйство"}}, {"value": "residential", "label": {"uzbek": "Turar joy qurilish", "russian": "Жилищное строительство"}}, {"value": "commercial", "label": {"uzbek": "Tijorat maqsadlari", "russian": "Коммерческие цели"}}, {"value": "industrial", "label": {"uzbek": "Sanoat", "russian": "Промышленность"}}]},
