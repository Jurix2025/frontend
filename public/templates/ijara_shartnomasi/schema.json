{
  "name": "Ijara shartnomasi",
  "name_en": "Lease Agreement",
  "category": "contracts",
  "languages": ["uzbek", "russian"],
  "fields": [
    {
      "id": "contract_type",
      "label": {
        "uzbek": "Shartnoma turi",
        "russian": "Тип договора"
      },
      "type": "select",
      "required": true,
      "options": [
        {
          "value": "residential",
          "label": {
            "uzbek": "Turar joy",
            "russian": "Жилое помещение"
          }
        },
        {
          "value": "commercial",
          "label": {
            "uzbek": "Tijorat maydoni",
            "russian": "Коммерческое помещение"
          }
        }
      ]
    },
    {
      "id": "landlord_name",
      "label": {
        "uzbek": "Ijaraga beruvchining F.I.Sh.",
        "russian": "Ф.И.О. арендодателя"
      },
      "type": "text",
      "required": true,
      "validation": {
        "minLength": 2,
        "maxLength": 200
      }
    },
    {
      "id": "landlord_passport",
      "label": {
        "uzbek": "Ijaraga beruvchining pasport seriyasi va raqami",
        "russian": "Серия и номер паспорта арендодателя"
      },
      "type": "text",
      "required": true,
      "validation": {
        "pattern": "^[A-Z]{2}[0-9]{7}$"
      }
    },
    {
      "id": "landlord_address",
      "label": {
        "uzbek": "Ijaraga beruvchining manzili",
        "russian": "Адрес арендодателя"
      },
      "type": "text",
      "required": true
    },
    {
      "id": "tenant_name",
      "label": {
        "uzbek": "Ijarachining F.I.Sh.",
        "russian": "Ф.И.О. арендатора"
      },
      "type": "text",
      "required": true,
      "validation": {
        "minLength": 2,
        "maxLength": 200
      }
    },
    {
      "id": "tenant_passport",
      "label": {
        "uzbek": "Ijarachining pasport seriyasi va raqami",
        "russian": "Серия и номер паспорта арендатора"
      },
      "type": "text",
      "required": true,
      "validation": {
        "pattern": "^[A-Z]{2}[0-9]{7}$"
      }
    },
    {
      "id": "tenant_address",
      "label": {
        "uzbek": "Ijarachining manzili",
        "russian": "Адрес арендатора"
      },
      "type": "text",
      "required": true
    },
    {
      "id": "property_address",
      "label": {
        "uzbek": "Ijara ob'ektining manzili",
        "russian": "Адрес объекта аренды"
      },
      "type": "text",
      "required": true
    },
    {
      "id": "property_area",
      "label": {
        "uzbek": "Umumiy maydoni (kv.m)",
        "russian": "Общая площадь (кв.м)"
      },
      "type": "number",
      "required": true,
      "validation": {
        "min": 1,
        "max": 10000
      }
    },
    {
      "id": "monthly_rent",
      "label": {
        "uzbek": "Oylik ijara haqi (so'm)",
        "russian": "Ежемесячная арендная плата (сум)"
      },
      "type": "number",
      "required": true,
      "validation": {
        "min": 0
      }
    },
    {
      "id": "deposit_amount",
      "label": {
        "uzbek": "Kafolat puli (so'm)",
        "russian": "Залоговая сумма (сум)"
      },
      "type": "number",
      "required": false,
      "validation": {
        "min": 0
      }
    },
    {
      "id": "payment_day",
      "label": {
        "uzbek": "To'lov sanasi (oyning kunlari)",
        "russian": "День оплаты (число месяца)"
      },
      "type": "number",
      "required": true,
      "validation": {
        "min": 1,
        "max": 31
      }
    },
    {
      "id": "start_date",
      "label": {
        "uzbek": "Shartnoma boshlanish sanasi",
        "russian": "Дата начала договора"
      },
      "type": "date",
      "required": true
    },
    {
      "id": "end_date",
      "label": {
        "uzbek": "Shartnoma tugash sanasi",
        "russian": "Дата окончания договора"
      },
      "type": "date",
      "required": true
    },
    {
      "id": "utilities_included",
      "label": {
        "uzbek": "Kommunal to'lovlar ijara haqiga kiritilganmi?",
        "russian": "Коммунальные услуги включены в аренду?"
      },
      "type": "boolean",
      "required": true
    }
  ],
  "ai_sections": [
    {
      "id": "ai_section_rights_obligations",
      "label": {
        "uzbek": "Tomonlarning huquq va majburiyatlari",
        "russian": "Права и обязанности сторон"
      },
      "section_number": "3",
      "description": {
        "uzbek": "Ijaraga beruvchi va ijarachining huquq va majburiyatlari",
        "russian": "Права и обязанности арендодателя и арендатора"
      },
      "prompt_guidance": "Generate detailed rights and obligations for both landlord and tenant in a lease agreement"
    },
    {
      "id": "ai_section_liability",
      "label": {
        "uzbek": "Javobgarlik",
        "russian": "Ответственность"
      },
      "section_number": "4",
      "description": {
        "uzbek": "Shartnoma shartlarini buzganligi uchun javobgarlik",
        "russian": "Ответственность за нарушение условий договора"
      },
      "prompt_guidance": "Generate liability clauses and penalties for breach of contract"
    },
    {
      "id": "ai_section_termination",
      "label": {
        "uzbek": "Shartnomani bekor qilish",
        "russian": "Расторжение договора"
      },
      "section_number": "5",
      "description": {
        "uzbek": "Shartnomani bekor qilish shartlari va tartibi",
        "russian": "Условия и порядок расторжения договора"
      },
      "prompt_guidance": "Generate contract termination conditions and procedures"
    },
    {
      "id": "ai_section_additional",
      "label": {
        "uzbek": "Qo'shimcha shartlar",
        "russian": "Дополнительные условия"
      },
      "section_number": "6",
      "description": {
        "uzbek": "Qo'shimcha shartlar va kelishuvlar",
        "russian": "Дополнительные условия и соглашения"
      },
      "prompt_guidance": "Generate any additional terms and conditions based on user requirements",
      "required": false
    },
    {
      "id": "ai_section_final",
      "label": {
        "uzbek": "Yakuniy qoidalar",
        "russian": "Заключительные положения"
      },
      "section_number": "7",
      "description": {
        "uzbek": "Yakuniy qoidalar va shartnoma tuzilishi",
        "russian": "Заключительные положения и составление договора"
      },
      "prompt_guidance": "Generate final provisions including number of copies, governing law, dispute resolution, etc."
    }
  ]
}
